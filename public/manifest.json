{
    "manifest_version": 3,
    "name": "PMT-ER",
    "version": "1.4.9",
    "action": {
        "default_popup": "index.html"
    },
    "background": { "service_worker": "background.js" },
    "permissions": ["webNavigation", "webRequest", "storage"],
    "icons": {
        "256": "PMT-Logo.png"
    },
    "description": "PMT - PDF viewer. This tool will say goodbye to the annoying pdf viewers in past paper websites"
}
